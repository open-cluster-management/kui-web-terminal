{
  "name": "@stolostron/kui-web-terminal-client",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "watch:source": "tsc --build . --watch",
    "compile": "npx tsc --build . && npx kui-babel && npx kui-prescan",
    "kill": "npm run kill:proxy; kill $(lsof -t -i:908${PORT_OFFSET-0}) > /dev/null 2> /dev/null || true",
    "watch:electron": "export CLIENT_HOME=$(pwd); npx webpack-dev-server --progress",
    "watch": "npm run kill; if [ ! -d node_modules/@kui-shell/build ]; then mkdir -p node_modules/@kui-shell/build; fi; concurrently -n PROXY,ES6,WEBPACK --kill-others 'npm run proxy' 'npm run watch:source' 'npm run watch:electron $WATCH_ARGS'",
    "proxy": "npx kui-run-proxy",
    "kill:proxy": "kill $(lsof -t -i:808${PORT_OFFSET-1}) > /dev/null 2> /dev/null || true",
    "build": "npm run compile && webpack --mode production",
    "build:css": "sass --no-source-map --style=compressed styles/mcm-kui.scss dist/webpack/mcm-kui.css",
    "postinstall": "npm run compile",
    "start": "WATCH_ARGS='-open' npm run watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "engines": {
    "npm": ">=6.9.0"
  },
  "devDependencies": {
    "@carbon/icons-react": "^10.33.0",
    "@kui-shell/builder": "~10.3.1",
    "@types/carbon-components": "^10.27.0",
    "@types/carbon-components-react": "^7.34.0",
    "@types/carbon__icons-react": "^10.31.0",
    "@types/node": "12.12.31",
    "@types/react": "^17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/react-tag-autocomplete": "6.1.0",
    "@types/yargs-parser": "20.2.0",
    "assert": "2.0.0",
    "browserify-zlib": "0.2.0",
    "carbon-components": "^10.37.0",
    "carbon-components-react": "^7.37.0",
    "carbon-icons": "^7.0.7",
    "concurrently": "6.1.0",
    "constants-browserify": "1.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "crypto-browserify": "3.12.0",
    "css-loader": "~5.2.4",
    "file-loader": "^6.0.0",
    "html-loader": "1.1.0",
    "html-webpack-plugin": "~5.3.1",
    "https-browserify": "1.0.0",
    "ignore-loader": "^0.1.2",
    "mini-css-extract-plugin": "^1.5.0",
    "os-browserify": "0.3.0",
