    "react-dom": "^17.0.2",
    "require-all": "^3.0.0",
    "sass": "~1.34.0",
    "sass-loader": "~11.0.1",
    "stream-browserify": "3.0.0",
    "stream-http": "3.2.0",
    "style-loader": "1.1.3",
    "svg-url-loader": "7.1.1",
    "typescript": "4.2.4",
    "url-loader": "^4.1.0",
